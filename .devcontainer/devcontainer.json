

{
    "image": "yzsimple/irc:fullv0.2.1",
    "containerEnv": {
        "datafolder":"/data",
        "workspace":"/workspaces"
    },
    "mounts": ["source=D://mkdata,target=/data,type=bind,consistency=cached"],

    "customizations": {
